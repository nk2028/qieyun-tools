{
	"name": "qieyun-tools",
	"private": true,
	"description": "A tool to find the Chinese characters by phonological expressions",
	"scripts": {
		"postinstall": "wget -nc 'https://cdn.jsdelivr.net/gh/ayaka14732/syyon-vencie@69bc015/texts/常用字頻序表.txt'",
		"start": "parcel src/index.html --open --port 8080",
		"build": "parcel build src/index.html --public-url /qieyun-tools",
		"lint": "eslint src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nk2028/qieyun-tools.git"
	},
